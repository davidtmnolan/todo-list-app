<tr id="<%= dom_id(todo_list) %>">
	<td><%= link_to todo_list.title, todo_list_todo_items_path(todo_list) %><br />
      <span class="created-at"><%= todo_list.created_at.strftime("%A, %b %d") %></span>
  </td>
	<td width="50px"><%= todo_list.todo_items.completed.count %> / <%= todo_list.todo_items.count %></td>
	<td width="10px"><%= link_to 'X', todo_list, method: :delete, data: { confirm: "Are you sure?" }, remote: true %></td>
</tr>
