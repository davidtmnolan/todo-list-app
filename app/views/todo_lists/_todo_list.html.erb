<tr id="<%= dom_id(todo_list) %>">
	<td><%= link_to todo_list.title, todo_list_todo_items_path(todo_list) %></td>
	<td width="20px"><%= todo_list.todo_items.completed.count %> / <%= todo_list.todo_items.count %></td>
  <td width="60px"><%= distance_of_time_in_words_to_now(todo_list.created_at) %></td>
	<td width="50px"><%= link_to 'X', todo_list, method: :delete, data: { confirm: "Are you sure?" }, remote: true %></td>
</tr>
