<tr id="<%= dom_id(todo_list) %>">
	<td><%= distance_of_time_in_words_to_now(todo_list.created_at) %></td>
	<td><%= link_to todo_list.title, todo_list_todo_items_path(todo_list) %></td>
	<td><%= todo_list.todo_items.completed.count %> of <%= todo_list.todo_items.count %></td>
	<td><%= link_to 'Destroy', todo_list, method: :delete, data: { confirm: "Are you sure?" }, remote: true %></td>
</tr>
